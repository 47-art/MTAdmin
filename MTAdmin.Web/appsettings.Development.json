{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AppConnection": "",
    "IdentityConnection": ""
  },
  "SiteAnalyticsOptions": {
    "ExcludeIPs": "",
    "ExcludeExtension": "",
    "ExcludePaths": "/css,/images,/js,/lib",
    "ExcludeLoopBack": true
  },
  "FileLogger": {
    "Options": {
      "FolderPath": "logs",
      "FilePath": "log_{date}.log"
    }
  }
}
